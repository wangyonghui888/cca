    <resultMap id="BaseResultMap" type="com.panda.sport.merchant.common.po.bss.SlotJackpotCfg">
        <id column="id" property="id"/>
        <result column="slot_machine_id" property="slotMachineId"/>
        <result column="place" property="place"/>
        <result column="number" property="number"/>
